{
  "name": "jwt-forward-auth",
  "version": "0.1.0",
  "description": "Use JWTs for Forward Auth",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "dev": "tsc && concurrently -i \"nodemon dist/index.js\" \"tsc -w\"",
    "format": "prettier 'src/**/*.ts' --write",
    "lint": "eslint",
    "prepare": "husky install"
  },
  "author": "Sebastiaan Besselsen <s.besselsen@sdu.nl>",
  "license": "MIT",
  "prettier": "@elseu/sdu-react-scripts-prettier",
  "dependencies": {
    "@sentry/node": "^8.0.0",
    "axios": "^1.6.8",
    "boolean": "^3.2.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "koa": "^2.15.3",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "match-url-wildcard": "0.0.5",
    "node-cache": "^5.1.2",
    "openid-client": "^5.6.5",
    "transliteration": "^2.3.5"
  },
  "devDependencies": {
    "@elseu/sdu-react-scripts-eslint": "^1.3.3",
    "@elseu/sdu-react-scripts-prettier": "^1.1.2",
    "@sentry/utils": "^7.114.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.15.0",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.2.0"
  }
}
